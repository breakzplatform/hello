<form name="Contato" method="POST" action="/contato/sucesso/" data-netlify="true" data-netlify-recaptcha="true">
  <p>
    <label>Your Name: <input class="block rounded-lg" type="text" name="name"/></label>
  </p>
  <p>
    <label>Your Email: <input type="email" name="email"/></label>
  </p>
  <p>
    <label>Message: <textarea name="message"></textarea>
    </label>
  </p>
  <p>
    <label>
      <span>Add file:</span>
      <input name="file" type="file"/>
    </label>
  </p>
  <div data-netlify-recaptcha="true"></div>
  <p>
    <button type="submit">Send</button>
  </p>
</form>